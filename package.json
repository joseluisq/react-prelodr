{
  "name": "react-prelodr",
  "version": "1.1.4",
  "description": "A React component based on Prelodr",
  "keywords": [
    "react",
    "prelodr",
    "preloader",
    "react-component"
  ],
  "license": "MIT",
  "repository": "joseluisq/react-prelodr",
  "author": {
    "name": "JosÃ© Luis Quintana",
    "url": "http://git.io/joseluisq"
  },
  "main": "./dist/react-prelodr.js",
  "dependencies": {
    "prelodr": "^2.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^2.13.1",
    "enzyme": "^2.8.2",
    "eslint": "^4.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.0",
    "jest-cli": "^20.0.4",
    "react-addons-test-utils": "^15.6.0",
    "readline-sync": "^1.4.7",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "start": "export NODE_ENV=development && webpack-dev-server -d --content-base ./app --hot --inline ./app/app.js",
    "mkdir:dist": "mkdir -p dist",
    "version": "npm run build",
    "build:lib": "export NODE_ENV=commonjs && babel index.js -o ./dist/react-prelodr.js --ignore 'test.js' --ignore 'app.js'",
    "build:min": "export NODE_ENV=umd && webpack --progress --colors -p index.js dist/react-prelodr.min.js",
    "build": "npm run mkdir:dist && npm run build:lib && npm run build:min",
    "lint": "eslint src",
    "test": "export NODE_ENV=testing && npm run lint && jest --coverage",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "peerDependencies": {
    "eslint": "4.1.0",
    "react": "^15.x",
    "react-dom": "^15.x"
  },
  "babel": {
    "env": {
      "testing": {
        "presets": [
          "es2015",
          "stage-0",
          "react"
        ]
      },
      "commonjs": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "stage-0",
          "react"
        ],
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      },
      "umd": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "stage-0",
          "react"
        ],
        "plugins": [
          "add-module-exports",
          "transform-es2015-modules-commonjs"
        ]
      },
      "development": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "stage-0",
          "react"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-jsx"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
